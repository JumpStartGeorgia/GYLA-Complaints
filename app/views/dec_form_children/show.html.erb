  <dd><%= @dec_form_child.describe_other_extra_env %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dec_form_id) %>:</strong></dt>
  <dd><%= @dec_form_child.dec_form_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              dec_form_children_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_dec_form_child_path(@dec_form_child), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              dec_form_child_path(@dec_form_child),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
