  <dd><%= @dec_form_child.disscusion_people %></dd>
  <dt><strong><%= model_class.human_attribute_name(:all_commission_memebers_attended) %>:</strong></dt>
  <dd><%= @dec_form_child.all_commission_memebers_attended %></dd>
  <dt><strong><%= model_class.human_attribute_name(:report_while_meeting) %>:</strong></dt>
  <dd><%= @dec_form_child.report_while_meeting %></dd>
